# MusicForge Environment Configuration
# Copy this file to .env and fill in your values

# LLM Provider Configuration
# Supported: openrouter, groq, ollama
LLM_PROVIDER=openrouter

# OpenRouter Configuration (recommended)
OPENROUTER_API_KEY=sk-or-your-key-here
OPENROUTER_MODEL=anthropic/claude-3-haiku

# Groq Configuration (alternative - fast inference)
GROQ_API_KEY=gsk_your-groq-key-here
GROQ_MODEL=llama-3.1-70b-versatile

# Ollama Configuration (local, free)
OLLAMA_ENDPOINT=http://localhost:11434
OLLAMA_MODEL=llama3.1

# gRPC Worker Configuration
GRPC_WORKER_HOST=localhost
GRPC_WORKER_PORT=50051

# API Configuration
ASPNETCORE_ENVIRONMENT=Development
API_PORT=8080

# Audio Output Settings
OUTPUT_FORMAT=mp3
OUTPUT_QUALITY=high
MAX_DURATION_SECONDS=300

# Model Settings (for Python worker)
MUSICGEN_MODEL=facebook/musicgen-small
BARK_MODEL=suno/bark-small
USE_GPU=true
